<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <ListViewPanel
    id="PolicySearch_ResultsLV"
    mode="Submission">
    <Require
      name="searchResults"
      type="gw.api.database.IQueryBeanResult&lt;PolicyPeriodSummary&gt;"/>
    <ExposeIterator
      flags="openUserRoleOwner"
      valueType="PolicyPeriodSummary"
      widget="PolicyPeriodSummaryIterator"/>
    <RowIterator
      checkBoxVisible="true"
      editable="false"
      elementName="policyPeriodSummary"
      hasCheckBoxes="true"
      id="PolicyPeriodSummaryIterator"
      pickValue="policyPeriodSummary"
      value="searchResults"
      valueType="gw.api.database.IQueryBeanResult&lt;entity.PolicyPeriodSummary&gt;">
      <ToolbarFlag
        condition="policyPeriodSummary.Job.CloseDate == null"
        name="openUserRoleOwner"/>
      <Row
        useArchivedStyle="policyPeriodSummary.TermArchived">
        <TextCell
          action="JobForward.go(policyPeriodSummary.Job)"
          id="SubmissionNumber"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.SubmissionNumber&quot;)"
          value="policyPeriodSummary.Job.JobNumber"/>
        <TextCell
          id="MainContact"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.MainContact&quot;)"
          value="policyPeriodSummary.InsuredDisplayName"/>
        <TextCell
          action="AccountFileDoRetrievalForward.go(policyPeriodSummary.AccountNumber)"
          available="perm.System.viewaccount"
          id="AccountNumber"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.AccountNumber&quot;)"
          value="policyPeriodSummary.AccountNumber"/>
        <TextCell
          enableSort="false"
          id="Product"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.Product&quot;)"
          value="policyPeriodSummary.Product"
          valueType="gw.api.productmodel.Product"/>
        <TextCell
          enableSort="false"
          id="SubmissionStatus"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.SubmissionStatus&quot;)"
          value="policyPeriodSummary.Job.DisplayStatus"/>
        <DateCell
          enableSort="false"
          id="SubmissionDate"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.SubmissionDate&quot;)"
          value="(policyPeriodSummary.Job typeis Submission)? policyPeriodSummary.Job.SubmissionDate : null"/>
        <TextCell
          enableSort="false"
          id="Producer"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.Producer&quot;)"
          value="policyPeriodSummary.ProducerName"/>
        <AltUserCell
          enableSort="false"
          id="Underwriter"
          label="DisplayKey.get(&quot;Web.PolicySearch.Results.Underwriter&quot;)"
          value="policyPeriodSummary.Job.Underwriter"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>