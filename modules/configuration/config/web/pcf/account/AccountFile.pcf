<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <LocationGroup
    id="AccountFile"
    infoBar="AccountFileInfoBar(account)"
    menuActions="AccountFileMenuActions(account)"
    parent="AccountForward()"
    title="DisplayKey.get(&quot;Web.AccountFile.LocationGroup.Title&quot;, account.AccountNumber)">
    <LocationEntryPoint
      signature="AccountFile(account : Account)"/>
    <Variable
      name="account"
      recalculateOnRefresh="true"
      type="Account"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Summary&quot;)"
      location="AccountFile_Summary(account)"
      shortcut="S"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Contacts&quot;)"
      location="AccountFile_Contacts(account)"
      shortcut="C"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Locations&quot;)"
      location="AccountFile_Locations(account)"
      shortcut="L"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Roles&quot;)"
      location="AccountFile_Roles(account)"
      shortcut="R"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.WorkOrders&quot;)"
      location="AccountFile_WorkOrders(account)"
      shortcut="W"/>
    <LocationRef
      location="SubmissionManager(account)"/>
    <LocationRef
      location="UnderwritingFiles(account)"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.RelatedAccounts&quot;)"
      location="AccountFile_RelatedAccounts(account)"
      shortcut="R"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Documents&quot;)"
      location="AccountFile_Documents(account)"
      shortcut="O"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Notes&quot;)"
      location="AccountFile_Notes(account)"
      shortcut="N"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Claims&quot;)"
      location="AccountFile_Claims(account)"
      shortcut="A"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.Billing&quot;)"
      location="AccountFile_Billing(account)"
      shortcut="B"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.AccountFile.History&quot;)"
      location="AccountFile_History(account)"
      shortcut="H"/>
  </LocationGroup>
</PCF>