package gw.riskassessment

/**
 * Class to configure Spotlight credentials. Spotlight Customers should change this
 * to provide valid credentials.
 */
@Export
class SpotlightCredentialProvider {

  static final var _spotlightUsername : String as Username = "valid riskassessment username"
  static final var _spotlightPassword : String as Password = "valid riskassessment password"

}